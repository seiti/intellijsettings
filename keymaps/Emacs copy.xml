<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt left" />
  </action>
  <action id="Diff.NextChange">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="n" />
  </action>
  <action id="Diff.PrevChange">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="p" />
  </action>
  <action id="EvaluateExpression">
    <keyboard-shortcut first-keystroke="alt f8" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="v" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="i" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="n" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="p" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="SelectInProjectView">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="t" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="f" />
  </action>
  <action id="com.dubreuia.core.SaveActionShortcutManager" />
